# Database connection string (for Prisma Data Proxy/Accelerate)
DATABASE_URL="prisma://accelerate.prisma-data.net/?api_key=YOUR_API_KEY"

# Direct connection URL for development (optional)
DIRECT_URL="postgresql://username:password@hostname:port/database"

# JWT secrets
JWT_SECRET="your-secret-key"
JWT_API_SECRET="your-api-secret-key"
JWT_EXPIRES_IN="7d"

# Server port
PORT=3001
